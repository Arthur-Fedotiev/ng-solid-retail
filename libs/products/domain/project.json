{
  "name": "products-domain",
  "projectType": "library",
  "sourceRoot": "libs/products/domain/src",
  "prefix": "sr",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/products/domain"],
      "options": {
        "jestConfig": "libs/products/domain/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/products/domain/**/*.ts",
          "libs/products/domain/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:products", "type:entities"]
}
